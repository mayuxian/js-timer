    "coverage": "vitest run --coverage --config ./vitest.config.ts"
  },
  "keywords": [
    "timer",
    "time",
    "countdown",
    "count-down",
    "setInterval",
    "setTimeout",
    "js-timer",
    "js-timers",
    "clock",
    "定时器",
    "倒计时"
  ],
  "homepage": "https://github.com/mayuxian/js-timers/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/mayuxian/js-timers.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/mayuxian/js-timers/issues"
  },
  "devDependencies": {
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vitest": "^0.25.6"
  },
  "exports": {
    ".": {
      "import": "./dist/js-timers.es.js",
